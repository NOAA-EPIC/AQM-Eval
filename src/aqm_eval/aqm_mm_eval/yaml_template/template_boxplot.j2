# This template is used to generate control_boxplot.yaml for MM application. Airnow surface observation is set as default.
{% include "mm_analysis.j2" %}

{% include "mm_model_defns.j2" %}

{% include "mm_obs.j2" %}

plots:
  pg5:
    type: "boxplot"
    fig_kwargs: {{ boxplot_fig_kwargs }}
    text_kwargs: {{ boxplot_text_kwargs }}
    domain_type: {{ boxplot_domain_type }}
    domain_name: {{ boxplot_domain_name }}
    data: {{ boxplot_dataset }}
    data_proc: {{ boxplot_data_proc }}
