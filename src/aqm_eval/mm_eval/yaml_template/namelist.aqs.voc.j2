stats_table_option: False
stats_table_kwargs:
  figsize: [7, 3]
  fontsize: 12.
  xscale: 1.4
  yscale: 1.4
  edges: "horizontal"
stats_domain_type: [
    "all",
    "epa_region", "epa_region", "epa_region", "epa_region", "epa_region",
    "epa_region", "epa_region", "epa_region", "epa_region", "epa_region",
]
stats_domain_name: [
    "CONUS",
    "R1", "R2", "R3", "R4", "R5",
    "R6", "R7", "R8", "R9", "R10",
]
stats_dataset: {{ package.mm_model_labels }}
