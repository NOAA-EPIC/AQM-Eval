model:
{% for mm_model in package.mm_models %}
  {{ mm_model.label }}: # model label
   files: {{ mm_model.link_alldays_path_template }}
   mod_type: {{ mm_model.cfg.type }}
   mod_kwargs: {{ mm_model.cfg.kwargs }}
   radius_of_influence: {{ mm_model.cfg.radius_of_influence}}
   mapping:
     {{ obs_label }}: {{ package.cfg.mapping }}
   variables: {{ mm_model.cfg.variables | tojson }}
   projection: {{ mm_model.cfg.projection | tojson }}
   plot_kwargs: {{ mm_model.cfg.plot_kwargs.model_dump() }}
{% endfor %}
