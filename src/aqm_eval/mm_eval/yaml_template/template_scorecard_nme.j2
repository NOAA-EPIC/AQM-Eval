# This template is used to generate control_scorecard_nme.yaml for MM application. Airnow surface observation is set as default.
{% include "mm_analysis.j2" %}

{% include "mm_model_defns.j2" %}

{% include "mm_obs.j2" %}

plots:
  pg74:
    type: "scorecard"
    fig_kwargs: {{ scorecard_fig_kwargs }}
    text_kwargs: {{ scorecard_text_kwargs }}
    domain_type: {{ scorecard_domain_type }}
    domain_name: {{ scorecard_domain_name }}
    region_name: {{ scorecard_region_type }}
    region_list: {{ scorecard_region_name }}
    urban_rural_name: {{ scorecard_urban_rural_name }}
    urban_rural_differentiate_value: {{ scorecard_urban_rural_value}}
    better_or_worse_method: {{ scorecard_eval_method }}
    data: {{ scorecard_dataset }}
    model_name_list: {{ scorecard_dataset_label }}
    data_proc: {{ scorecard_data_proc }}
