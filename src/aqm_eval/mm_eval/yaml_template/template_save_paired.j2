# This template is used to generate control_save_paired.yaml for MM application. Airnow surface observation is set as default.
analysis:
  start_time: {{ start_time }} # yyyy-mm-dd-HH:MM:SS UTC
  end_time: {{ end_time }} # yyyy-mm-dd-HH:MM:SS UTC
  output_dir: {{ output_dir }}
  debug: {{ debug_option }}
  save:
    paired:
      method: {{ paired_format }}
      predix: {{ paired_predix }}
      data: {{ paired_save_data }}

{% include "mm_model_defns.j2" %}

